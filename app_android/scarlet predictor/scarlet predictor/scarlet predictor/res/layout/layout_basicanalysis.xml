<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@drawable/hula">

    <RelativeLayout
        android:layout_width="387dp"
        android:layout_height="wrap_content"
        android:layout_x="103dp"
        android:layout_y="443dp" >
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="202dp"
        android:layout_height="54dp"
        android:layout_x="217dp"
        android:layout_y="526dp" >
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/graphlayout"
        android:layout_width="546dp"
        android:layout_height="381dp"
        android:layout_x="30dp"
        android:layout_y="100dp"
        android:orientation="vertical" >

    </LinearLayout>

    <RelativeLayout
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:layout_x="-3dp"
        android:layout_y="1dp" >

        <Button
            android:id="@+id/button2"
           android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/dataupdate"
            android:text="Button" 
             android:visibility="invisible"
             />

        <Button
            android:id="@+id/select"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="20dp"
            android:layout_marginTop="10dp"
           
            android:background="@drawable/select"
            />

        <Button
            android:id="@+id/back"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
          
            android:background="@drawable/back" />

        <Button
            android:id="@+id/dataupdate"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/back"
            android:text="Button" 
            android:visibility="invisible"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="299dp"
        android:layout_height="190dp"
        android:layout_x="141dp"
        android:layout_y="650dp" >

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Ask"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/textView3"
            android:text="Open"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24dp" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/closevalue"
            android:text="Volume"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24dp" />

        <TextView
            android:id="@+id/TextView07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView01"
            android:text="Bid"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24sp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/bidvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView07"
            android:layout_alignBottom="@+id/TextView07"
            android:layout_alignParentRight="true"
            android:text="N/A"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24dp" 
            android:visibility="invisible"/>
        
        <TextView
            android:id="@+id/volumevalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/bidvalue"
            android:layout_alignParentRight="true"
            android:text="N/A"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24dp" />

        <TextView
            android:id="@+id/openvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView02"
            android:layout_alignBottom="@+id/TextView02"
            android:layout_alignParentRight="true"
            android:text="N/A"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/askvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/openvalue"
            android:layout_alignParentRight="true"
            android:text="N/A"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView02"
            android:text="Close"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24dp" />

        <TextView
            android:id="@+id/closevalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/openvalue"
            android:text="N/A"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="24dp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_x="226dp"
        android:layout_y="603dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="32sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/symbol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="120dp"
        android:layout_y="530dp"
        android:text="GOOG: "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="48sp" />

    <TextView
        android:id="@+id/price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="351dp"
        android:layout_y="493dp"
        android:background="#ffdc143c"
        android:text="PRICE"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="48dp" 
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="190dp"
        android:layout_y="64dp"
        android:text="REAL TIME ASKPRICE"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</AbsoluteLayout>
